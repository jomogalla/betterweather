<?xml version="1.0"?>
            <!DOCTYPE hibernate-mapping PUBLIC
                "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping>
	<!-- replace environments with the table to be used
		the table must have fields: id=INT, OS=VARCHAR(30), OS_VERSION=VARCHAR(30), notes=LONGTEXT -->
	<class name="model.Ide" table="cities">
		<id name="id" column="geoID">
			<generator class="increment" />
		</id>
		
		<property name="city" type="string" column="city" />
		<property name="state" type="string" column="state" />
		<property name="longitude" type="double" column="longitude" />
		<property name="latitude" type="double" column="latitude" />
	</class>
 
</hibernate-mapping>

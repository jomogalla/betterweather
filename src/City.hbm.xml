<?xml version="1.0"?>
            <!DOCTYPE hibernate-mapping PUBLIC
                "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping>
	<class name="model.City" table="cities">
		<id name="id" column="geoID">
			<generator class="increment"/>
		</id>
		
		<property name="city" type="string" column="city" />
		<property name="state" type="string" column="state" />
		<property name="longitude" type="double" column="longitude" />
		<property name="latitude" type="double" column="latitude" />
		<!--<property name="distance" type="double" column="distance" />-->
	</class>
 
</hibernate-mapping>
